{
	"name": "Switchpipeline",
	"properties": {
		"activities": [
			{
				"name": "Swith Metadata",
				"type": "GetMetadata",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "para_data_set",
						"type": "DatasetReference",
						"parameters": {
							"DS_filesystem": {
								"value": "@pipeline().parameters.switch_filesystem",
								"type": "Expression"
							},
							"DS_directory": {
								"value": "@pipeline().parameters.switch_dir",
								"type": "Expression"
							},
							"DS_filename": {
								"value": "@pipeline().parameters.switch_filename",
								"type": "Expression"
							}
						}
					},
					"fieldList": [
						"childItems",
						"columnCount",
						"itemName",
						"itemType"
					],
					"storeSettings": {
						"type": "AzureBlobFSReadSettings",
						"recursive": true,
						"enablePartitionDiscovery": false
					},
					"formatSettings": {
						"type": "DelimitedTextReadSettings"
					}
				}
			},
			{
				"name": "Get Metadata1",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "Swith Metadata",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('Get Metadata1').output.childItems",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "Switch1",
							"type": "Switch",
							"dependsOn": [],
							"userProperties": [],
							"typeProperties": {
								"on": {
									"value": "@split(item().name,'.' )[1]",
									"type": "Expression"
								},
								"cases": [
									{
										"value": "txt"
									}
								]
							}
						}
					]
				}
			}
		],
		"parameters": {
			"switch_filesystem": {
				"type": "String"
			},
			"switch_dir": {
				"type": "String"
			},
			"switch_filename": {
				"type": "String"
			}
		},
		"annotations": []
	}
}